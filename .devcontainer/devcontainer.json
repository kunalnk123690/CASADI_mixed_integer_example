{
    "name": "Development Container for Building Casadi Codegen Projects",
    "privileged": true,
    "remoteUser": "Casadi_mixed_integer",
    "build": {
        "dockerfile": "Dockerfile"
    },
    "workspaceFolder": "/home/Casadi_mixed_integer/Casadi_mixed_integer_ws",
    "workspaceMount": "source=${localWorkspaceFolder},target=/home/Casadi_mixed_integer/Casadi_mixed_integer_ws,type=bind",
    "customizations": {
        "vscode": {
            "extensions":[
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-themes",
                "ms-vscode.cmake-tools",
                "ms-python.python",
                "ms-python.vscode-pylance"
            ]
        }
    },
    "containerEnv": {
        "CMAKE_GENERATOR": "Ninja"
    }
}